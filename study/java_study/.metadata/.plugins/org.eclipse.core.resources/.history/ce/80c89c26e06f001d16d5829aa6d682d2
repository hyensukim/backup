package day14.sub02;

import java.io.IOException;
import java.io.File;
import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) throws IOException{
		
		Scanner sc = new Scanner(new File("data.txt"));
		while(true) {
			int num = sc.nextInt();
			System.out.println(num);
			break;
		}
		
	}

}

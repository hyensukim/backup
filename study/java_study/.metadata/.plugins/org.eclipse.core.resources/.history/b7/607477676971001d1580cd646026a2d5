package day16;

import java.time.*;
import static java.time.temporal.ChronoField.*;

public class Ex05 {

	public static void main(String[] args) {
		LocalDateTime ldt1 = LocalDateTime.now();
		System.out.println(ldt1);
		
		LocalDateTime ldt2 = ldt1.with(YEAR,2023);
		System.out.println(ldt2);
		
	}

}
